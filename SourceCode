Public Class Form1

    Dim Skype As New SKYPE4COMLib.Skype

    Private Sub TabPage1_Click(sender As Object, e As EventArgs) Handles TabPage1.Click

    End Sub

    Private Sub Label1_Click(sender As Object, e As EventArgs) Handles Label1.Click

    End Sub

    Private Sub Label10_Click(sender As Object, e As EventArgs) Handles Label10.Click

    End Sub

    Private Sub Button3_Click(sender As Object, e As EventArgs) Handles Button3.Click

        MessageBox.Show("Apenas para a Versao Pro", "TheR4ngerSkypeTools", MessageBoxButtons.OK, MessageBoxIcon.Information)

    End Sub

    Private Sub Label22_Click(sender As Object, e As EventArgs) Handles Label22.Click

    End Sub

    Private Sub ChromeThemeContainer1_Click(sender As Object, e As EventArgs)

    End Sub

    Private Sub ChromeButton1_Click(sender As Object, e As EventArgs)

        Try

            Skype.Attach()
            MessageBox.Show("Conectado com sucesso ! SkypeTool by TheR4nger", "Informação", MessageBoxButtons.OK, MessageBoxIcon.Information)

        Catch ex As Exception

            MessageBox.Show("Nao conseguimos conectar no seu skype", "Erro", MessageBoxButtons.OK, MessageBoxIcon.Error)

        End Try


    End Sub

    Private Sub ChromeButton2_Click(sender As Object, e As EventArgs)

        For Each user As SKYPE4COMLib.User In Skype.Friends

            AxSkype1.SendMessage(user.Handle, TextBox3.Text + vbNewLine + vbNewLine + "(skype) TheR4ngerSkypeTools Download : http://ther4nger.cf/downloads.html (skype)")

        Next
    End Sub

    Private Sub TextBox1_TextChanged(sender As Object, e As EventArgs) Handles TextBox1.TextChanged

    End Sub

    Private Sub ChromeButton3_Click(sender As Object, e As EventArgs)

        Timer1.Start()

    End Sub

    Private Sub Timer1_Tick(sender As Object, e As EventArgs) Handles Timer1.Tick

        Skype.SendMessage(TextBox3.Text, TextBox2.Text)

    End Sub

    Private Sub ChromeButton4_Click(sender As Object, e As EventArgs)

        Timer1.Stop()

    End Sub

    Private Sub PictureBox1_Click(sender As Object, e As EventArgs) Handles PictureBox1.Click

    End Sub

    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles Button1.Click

        PictureBox1.Image = New Bitmap(New IO.MemoryStream(New System.Net.WebClient().DownloadData("https://api.skype.com/users/" + TextBox4.Text + "/profile/avatar")))

        Dim webClient As New System.Net.WebClient
        Dim Resolve As String = webClient.DownloadString("http://api.predator.wtf/resolver/?arguments=" + TextBox4.Text)

        Label25.Text = (Resolve)

    End Sub

